# Activar RewriteEngine
RewriteEngine on
 
# Reescribir la URL solicitada por el usuario
#   Entrada:  user/ID/
#   Salida: user.php?id=ID
RewriteRule ^user/(\d+)/?$ user.php?id=$1

# Reescribir la URL solicitada por el usuario
#   Entrada:  user
#   Salida: user.php
RewriteRule ^user?$ user.php

# Reescribir la URL solicitada por el usuario
#   Entrada:  movies/ID/
#   Salida: movies.php?id=ID
RewriteRule ^movies/(\d+)/?$ movies.php?id=$1

# Reescribir la URL solicitada por el usuario
#   Entrada:  movies
#   Salida: movies.php
RewriteRule ^movies?$ movies.php


